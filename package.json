{
  "name": "code-picker",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@9.5.0",
  "description": "Extract the component code needed from the component library and add it to your local project",
  "author": "proc07",
  "license": "MIT",
  "homepage": "https://github.com/proc07/code-picker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proc07/code-picker.git"
  },
  "bugs": "https://github.com/proc07/code-picker/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "bin": {
    "cp": "bin/cp.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "cp": "code-picker --repo https://github.com/element-plus/element-plus/tree/dev/packages/components/form/index.ts --out ./examples"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "node-fetch": "^3.3.2",
    "picocolors": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.14.11",
    "bumpp": "^9.4.1",
    "code-picker": "^0.0.3",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
